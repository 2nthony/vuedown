{
  "name": "vuedown",
  "description": "üìùYour Markdown is your Vue-Component.",
  "version": "1.2.0",
  "license": "MIT",
  "files": [
    "*.ts",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "evillt/vuedown"
  },
  "author": "evillt <ijoec123@gmail.com> (https://evila.me)",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "example": "dvan example --dev --config example/dvan.config.js",
    "lint": "xo"
  },
  "dependencies": {
    "loader-utils": "^1.2.3",
    "markdown-it": "^8.4.2"
  },
  "devDependencies": {
    "dvan": "^2.3.7",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-rem": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "^1.15.2",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8",
    "xo": "^0.23.0"
  },
  "xo": {
    "extends": [
      "rem",
      "plugin:prettier/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
